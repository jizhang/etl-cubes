{
  "cubes": [{
    "name": "da_etl_job_cube",
    "label": "ETL脚本运行明细",
    "dimensions": ["cal_dt", "module_name", "owner", "category", "layer", "level", "job_type", "operation", "status", "duration_type", "7day_average_percent_type"],
    "measures": [{
      "name": "duration",
      "label": "运行时长",
      "aggregates": ["sum", "min", "max", "avg"]
    }, {
      "name": "7day_average_duration",
      "label": "7天平均时长",
      "aggregates": ["sum", "min", "max", "avg"]
    }, {
      "name": "7day_average_percent",
      "label": "时长增长",
      "aggregates": ["min", "max", "avg"]
    }],
    "aggregates": [{
      "name": "record_count",
      "label": "脚本数",
      "function": "count"
    }],
    "mappings": {
      "cal_dt.year": {"column": "cal_dt", "extract": "year"},
      "cal_dt.month": {"column": "cal_dt", "extract": "month"},
      "cal_dt.day": {"column": "cal_dt", "extract": "day"}
    }
  }],

  "dimensions": [{
    "name": "cal_dt",
    "label": "日期",
    "role": "time",
    "levels": [{
      "name": "year",
      "label": "年",
      "info": {
        "cv-datefilter-field": "year"
      }
    }, {
      "name": "month",
      "label": "月",
      "info": {
        "cv-datefilter-field": "month"
      }
    }, {
      "name": "day",
      "label": "日",
      "info": {
        "cv-datefilter-field": "day"
      }
    }],
    "info": {
      "cv-datefilter": true
    }
  }, {
    "name": "module_name",
    "label": "脚本名称"
  }, {
    "name": "owner",
    "label": "创建人"
  }, {
    "name": "category",
    "label": "业务线"
  }, {
    "name": "layer",
    "label": "层次"
  }, {
    "name": "level",
    "label": "级别"
  }, {
    "name": "job_type",
    "label": "脚本类型"
  }, {
    "name": "operation",
    "label": "当日操作"
  }, {
    "name": "status",
    "label": "运行状态"
  }, {
    "name": "duration_type",
    "label": "运行时间范围"
  }, {
    "name": "7day_average_percent_type",
    "label": "运行时间浮动范围"
  }]
}
